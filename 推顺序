#include<stdio.h>
void scc(int *p,int n,int m);
 int main()
 {
 	
 	int a[20],i,n,m;
 	printf("请输入数的个数(<20):");
 	scanf("%d",&n);
 	printf("请输入%d个数:",n);
 	for(i=0;i<n;i++)
 	scanf("%d",&a[i]);
 	printf("请输入移动位置数:");
 	scanf("%d",&m);
 	scc(a,n,m);
 	for(i=0;i<n;i++)
 	printf("%3d",a[i]);
 	return 0;
 }
 void scc(int *p,int n,int m)
 {
 	int i,j,temp,cot=0;
 	while(cot!=m){
 		temp=*(p+n-1);
 		for(i=n-1;i>=1;i--)
 		{
 			*(p+i)=*(p+i-1);
		 }
		 *p=temp;
		 cot++;
	 }
 }
